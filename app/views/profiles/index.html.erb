  <button id="demo-menu-lower-left"
        class="mdl-button mdl-js-button mdl-button--icon">
        <i class="material-icons">more_vert</i>
      </button>
      <ul class="mdl-menu mdl-menu--bottom-left mdl-js-menu mdl-js-ripple-effect"
      for="demo-menu-lower-left">
      <li class="mdl-menu__item">Profiles</li>
      <li class="mdl-menu__item"><a href="../../pages/locations.html">Locations</a></li>
      <li class="mdl-menu__item"><a href="../../pages/resources.html">Resources</a></li>
    </ul> 

  <div id="all-profiles">
  <p id="notice"><%= notice %></p>

  <h1>All Profiles</h1>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Alma</th>
        <th>Mater</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @profiles.each do |profile| %>
        <tr>
          <td><%= profile.name %></td>
          <td><%= profile.email %></td>
          <td><%= profile.alma %></td>
          <td><%= profile.mater %></td>
          <td><%= link_to 'Show', profile %></td>
          <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
          <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <div id="new-prof-button" style="width:200px;">

  <%= link_to 'New Profile', new_profile_path, class: "waves-effect waves-light btn", style: "width: 100%; margin-top: 25px" %>
  </div>
  </div>
